{
    "igvm": {
        "vanadium": {
            "output": "coconut-test-vanadium.igvm",
            "platforms": [
                "snp",
                "tdp"
            ],
            "policy": "0x30000",
            "measure": "print",
            "check-kvm": true,
            "measure-native-zeroes": true
        }
    },
    "kernel": {
        "svsm": {
            "type": "make",
            "output_file": "bin/test-kernel.elf"
        },
        "bldr": {
            "manifest": "boot/bldr/Cargo.toml",
            "binary": true,
            "objcopy": "binary"
        },
        "tdx-stage1": {
            "type": "make",
            "output_file": "bin/stage1-trampoline",
            "objcopy": "binary"
        }
    },
    "firmware": {},
    "fs": {
        "modules": {}
    }
}
